{
  "name": "generals.io",
  "version": "1.0.0",
  "description": "Bot for generals.io",
  "scripts": {
    "build": "npm run copy && npm run compile",
    "copy": "rsync -a --exclude '*.ts' src/ lib/",
    "compile": "tsc",
    "dev": "npm run build && npm start",
    "selenium": "java -jar selenium-server-standalone-3.0.1.jar",
    "start": "node lib/index.js",
    "test": "rm -rf lib && npm run build && mocha 'lib/**/*.spec.js'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bluebird": "3.4.7",
    "chai": "3.5.0",
    "deep-freeze": "0.0.1",
    "lodash": "4.17.2",
    "vantage": "1.7.0",
    "webdriverio": "4.5.0"
  },
  "devDependencies": {
    "@types/chai": "3.4.34",
    "@types/jsdom": "2.0.29",
    "@types/lodash": "4.14.43",
    "@types/mocha": "2.2.33",
    "@types/sinon": "1.16.33",
    "@types/webdriverio": "4.4.0",
    "jsdom": "9.9.1",
    "mocha": "3.2.0",
    "sinon": "1.17.6",
    "tslint": "4.1.0",
    "typescript": "2.1.4"
  }
}
